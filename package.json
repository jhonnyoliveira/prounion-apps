{
  "name": "prounion-apps-gateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "ProUnion Software",
  "scripts": {
    "test": "vitest",
    "lint": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "format": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "build": "rimraf dist && tsc --project tsconfig.build.json && resolve-tspaths",
    "dist": "rimraf bin && npm run build && javascript-obfuscator ./dist --output ./bin",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "start": "node -r dotenv/config bin/src/index.js"
  },
  "license": "ISC",
  "dependencies": {
    "compression": "1.7.5",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "helmet": "8.0.0",
    "javascript-obfuscator": "4.1.1",
    "jsonwebtoken": "9.0.2",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "resolve-tspaths": "0.8.22",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.9.3",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-node-dev": "2.0.0",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "2.1.5"
  }
}
